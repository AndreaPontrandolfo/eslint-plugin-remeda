{
  "name": "eslint-plugin-remeda",
  "version": "0.0.0-development",
  "author": "Andrea Pontrandolfo <andrea.pontra@gmail.com>",
  "description": "ESLint plugin for Remeda library.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc",
    "lint": "eslint",
    "knip": "knip",
    "publint": "publint",
    "test:watch": "vitest --ui",
    "test": "vitest run",
    "attw": "attw --pack .",
    "qa": "pnpm typecheck && pnpm test && pnpm knip && pnpm publint && attw",
    "nuke": "rm -rf node_modules pnpm-lock.yaml",
    "update:eslint-docs": "eslint-doc-generator",
    "lint:eslint-docs": "pnpm update:eslint-docs -- --check",
    "semantic-release": "pnpm build && semantic-release"
  },
  "files": [
    "README.md",
    "dist",
    "docs"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreaPontrandolfo/eslint-plugin-remeda.git"
  },
  "homepage": "https://github.com/AndreaPontrandolfo/eslint-plugin-remeda",
  "bugs": "https://github.com/AndreaPontrandolfo/eslint-plugin-remeda/issues",
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@sherifforg/cli": "^8.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "eslint": "9.24.0",
    "eslint-config-sheriff": "^27.0.0",
    "eslint-doc-generator": "^2.1.2",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-vitest-rule-tester": "^2.2.0",
    "lodash-es": "^4.17.21",
    "knip": "^5.49.0",
    "prettier": "^3.3.2",
    "publint": "^0.3.10",
    "semantic-release": "^24.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.5.2",
    "@typescript-eslint/utils": "^8.29.0",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.9.0",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslint plugin",
    "eslintplugin",
    "remeda",
    "functional",
    "fp"
  ],
  "license": "MIT"
}
